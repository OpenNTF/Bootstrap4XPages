    <renderer>
      <component-family>javax.faces.Panel</component-family>
      <renderer-type>org.openntf.xsp.bootstrap.tagcloud.BootstrapTagCloud</renderer-type>
      <renderer-class>org.openntf.xsp.bootstrap.renderkit.html_extended.extlib.tagcloud.BootstrapTagCloudRenderer</renderer-class>
    </renderer>

