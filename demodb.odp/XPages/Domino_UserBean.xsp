<?xml version="1.0" encoding="UTF-8"?>
<xp:view xmlns:xp="http://www.ibm.com/xsp/core" xmlns:xe="http://www.ibm.com/xsp/coreex"
	xmlns:xc="http://www.ibm.com/xsp/custom">
	<xc:OneUILayout id="cc4cconeuilayout"  navigationPath="/Domino/UserBean">
		<xp:this.facets>
			<xp:panel xp:key="LeftColumn" id="panel4">
				<xc:LeftDomino id="cc4ccLeftDomino" ></xc:LeftDomino>
			</xp:panel>
		</xp:this.facets>
		<xp:panel>
			<xp:label value="This demonstrates the Domino userBean:" id="label5"
				style="font-weight:bold">
			</xp:label>
			<xp:br></xp:br>
			<xp:br></xp:br>
			<xp:table>
                <xp:tr>
                    <th>Field</th>
                    <th>Value</th>
                </xp:tr>
				<xp:tr>
					<xp:td>Picture</xp:td>
					<xp:td>
						<xp:image url="#{userBean.thumbnailUrl}" id="image1"></xp:image>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Owner (owner)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField20" value="#{userBean.owner}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Viewer (viewer)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField21" value="#{userBean.viewer}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Common Name (displayName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField4" value="#{userBean.displayName}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Common Name (commonName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField8" value="#{userBean.commonName}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Canonical Name (id)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField2" value="#{userBean.id}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Canonical Name (canonicalName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField7" value="#{userBean.canonicalName}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Distinguished Name (distinguishedName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField9" value="#{userBean.distinguishedName}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Abbreviated Name (abbreviatedName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField3" value="#{userBean.abbreviatedName}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>Effective User Name (effectiveUserName)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField10" value="#{userBean.effectiveUserName}">
						</xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td colspan="2">
						<xp:label value="ACL Privileges"
							id="label2" style="font-weight:bold">
						</xp:label>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>DBACL_CREATE_DOCS (canCreateDocs)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField11" value="#{userBean.canCreateDocs}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>DBACL_DELETE_DOCS (canDeleteDocs)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField12" value="#{userBean.canDeleteDocs}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>DBACL_CREATE_PRIV_AGENTS
						(canCreatePrivAgents)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField13"
							value="#{userBean.canCreatePrivAgents}"></xp:text>
					</xp:td>
				</xp:tr>

				<xp:tr>
					<xp:td>DBACL_CREATE_PRIV_FOLDERS_VIEWS
						(canCreatePrivFoldersViews)</xp:td>
					<xp:td>
						<xp:text escape="true" id="computedField14"
							value="#{userBean.canCreatePrivFoldersViews}"></xp:text>
					</xp:td>
				</xp:tr>
				<xp:tr>
					<xp:td>DBACL_CREATE_SHARED_FOLDERS_VIEWS
